<%- include('include/head.ejs') %>
    <link rel="stylesheet" href="/css/home.css">
    <link rel="stylesheet" href="/css/top_nav_white.css">
    <link rel="stylesheet" href="/css/side_nav.css">
    <link rel="stylesheet" href="/css/items_home.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css">
    <link rel="stylesheet" href="/css/baap.css">
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/css/transition.css">


    </head>

    <body>
        <div id="main-body">

            <div id="header">
                <%- include('include/first-row.ejs')%>
                    <div id="navigation">

                        <%- include('include/top_nav.ejs')%>
                            <%-include('include/side_nav.ejs')%>

                    </div>

            </div>
            <div class="grid-container">
                <% types.forEach(type =>{ %>
                    <div class="grid-item">
                        <div class="col-md-4">
                            <div class="profile-card-6">
                                <a href="<%= type.link %>?page=1&row=nan"><img src='<%= type.Wallpaper %>' class="img img-responsive"></a>
                                <div class="profile-name">
                                    <%= type.title %>
                                </div>
                                <div class="profile-overview">
                                    <div class="profile-overview">
                                        <div class="row text-center">
                                            <div class="col-xs-4">

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>
            </div>



        </div>

        <%- include('include/end.ejs')%>